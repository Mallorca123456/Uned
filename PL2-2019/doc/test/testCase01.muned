MODULE seis;

  VAR resta : INTEGER;
  		a1:INTEGER;
  		b1:INTEGER;

  PROCEDURE Restar(a,b: INTEGER) : INTEGER;
  	VAR s: INTEGER;
 	BEGIN
 		s := a-b;
 		RETURN s; 	
 	END Restar; 
  
  BEGIN
    a1:=10;
    b1:=3;
    resta:=Restar(a1,b1);
    WRITEINT(a1);
    WRITEINT(b1);
    WRITEINT(resta);

  END seis;
